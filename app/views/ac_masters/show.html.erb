<%- model_class = AcMaster -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @ac_master.code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @ac_master.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cd_division) %>:</strong></dt>
  <dd><%= @ac_master.cd_division %></dd>
  <dt><strong><%= model_class.human_attribute_name(:level) %>:</strong></dt>
  <dd><%= @ac_master.level %></dd>
  <dt><strong><%= model_class.human_attribute_name(:upper_code) %>:</strong></dt>
  <dd><%= @ac_master.upper_code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:detail) %>:</strong></dt>
  <dd><%= @ac_master.detail %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ac_masters_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ac_master_path(@ac_master), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ac_master_path(@ac_master),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
